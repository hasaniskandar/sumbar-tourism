<div class="container">
  <div class="row">
    <div class="span12">
      <ul class="thumbnails">
        <li class="span8">
          <%= render "shared/tourism_article_content" %>
        </li>

        <li class="span4">
          <%= render "shared/tourism_article_sub_content",
            :article => TourismArticle.last,
            :event => Event.last,
            :folktales => Folktale.order('created_at DESC').limit(5),
            :tips_tricks => TipsTrick.order('created_at DESC').limit(5)
          %>
        </li
      </ul>

    </div>
  </div>
</div>