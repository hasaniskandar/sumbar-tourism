<h1>Listing sumbar_contents</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Content</th>
    <th>Type</th>
    <th>User</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @sumbar_contents.each do |sumbar_content| %>
  <tr>
    <td><%= sumbar_content.title %></td>
    <td><%= sumbar_content.content %></td>
    <td><%= sumbar_content.type %></td>
    <td><%= sumbar_content.user_id %></td>
    <td><%= link_to 'Show', sumbar_content %></td>
    <td><%= link_to 'Edit', edit_sumbar_content_path(sumbar_content) %></td>
    <td><%= link_to 'Destroy', sumbar_content, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

